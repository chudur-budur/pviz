}

@inproceedings{filipicTaxonomyMethodsVisualizing2018,
  address = {New York, NY, USA},
  series = {GECCO '18},
  title = {A {{Taxonomy}} of {{Methods}} for {{Visualizing Pareto Front Approximations}}},
