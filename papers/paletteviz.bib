  pages = {386-402},
  file = {/media/khaled/data/research/zotero/storage/ECF5LP3P/He and Yen - 2016 - Visualization and Performance Metric in Many-Objec.pdf;/media/khaled/data/research/zotero/storage/6VUT3EQ9/7219451.html}
